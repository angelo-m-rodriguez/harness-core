<configuration name="CVNGApp"
               type="BlazeCommandRunConfigurationType"
               factoryName="Bazel Command">
    <blaze-settings handler-id="BlazeJavaRunConfigurationHandlerProvider"
                    kind="java_binary"
                    debug_port="6007"
                    blaze-command="run"
                    keep-in-sync="true">
        <blaze-target>//300-cv-nextgen:module</blaze-target>
        <blaze-user-exe-flag>server</blaze-user-exe-flag>
        <blaze-user-exe-flag>$PROJECT_DIR$/../300-cv-nextgen/cv-nextgen-config.yml</blaze-user-exe-flag>
    </blaze-settings>
    <method v="2">
        <option name="Blaze.BeforeRunTask" enabled="true"/>
    </method>
</configuration>